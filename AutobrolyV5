function Grappear()
pcall(function()
    local LPChar = game:GetService("Players").LocalPlayer.Character
    LPChar:FindFirstChildOfClass("Humanoid"):EquipTool(game:GetService("Players").LocalPlayer.Backpack[Dragon])
    LPChar[Dragon].Activator.Flip:Destroy()
    LPChar[Dragon]:Activate()
    MoveListo = true
end)
wait(0.5)
pcall(function()
    local LPChar = game:GetService("Players").LocalPlayer.Character
    LPChar:FindFirstChildOfClass("Humanoid"):EquipTool(game:GetService("Players").LocalPlayer.Backpack[Dragon])
    LPChar[Dragon]:Activate()
    MoveListo = true
end)
end
Grappear()
end
