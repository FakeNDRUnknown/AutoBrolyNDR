spawn(function()
    pcall(function()
        if true then wait(2)
           if game.PlaceId ~= 2050207304 then
                 if not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Dragon Crush") or not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Dragon Throw") then
                    game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Requirements", Text = "You need have got Dragon Crush and Dragon Thorw :v"})
                    game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Requerimientos", Text = "Necesitas tener Dragon Crush y Dragon Throw :v"})
                 elseif game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Dragon Crush") or game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Dragon Throw") then
              game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Creador", Text = "NabilDr#4888"})
              game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Owner", Text = "NabilDr#4888"})
           end
        end   
     end   
     
        if true then
           if game.PlaceId == 2050207304 then
              game:GetService("RunService").RenderStepped:connect(function() 
                 pcall(function()
                   char.Action:Destroy() 
                   char.Activity:Destroy() 
                   char.Attacking:Destroy()
                 end)
              end)
           end
        end
        if true then
        if game.PlaceId == 2050207304 then
           game:GetService("RunService").RenderStepped:connect(function() 
              pcall(function()
                char.Action:Destroy() 
                char.Activity:Destroy() 
                char.Attacking:Destroy()
              end)
           end)
        end
     end
  end)
end)
